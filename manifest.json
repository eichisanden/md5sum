{
	"name": "md5sum",
	"version": "1.2",
	"manifest_version": 2,
	"description": "This extension output md5 message digest by right-clicking on link or drag-and-drop files.",
	"background": {
		"persistent": false,
		"scripts": [
			"js/lib/md5.js",
			"js/background.js"
		]
	},
	"browser_action": {
		"default_title": "md5sum"
	},
	"icons": {
			"16": "images/icon_016.png",
			"48": "images/icon_048.png",
			"128": "images/icon_128.png"
	},
	"content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
	"permissions": [
		"contextMenus",
		"tabs",
		"http://*/*",
		"https://*/*"
	]
}
