{
	"name": "md5sum",
	"version": "0.1",
	"manifest_version": 2,
	"description": "md5sum",
	"background": {
		"persistent": false,
		"scripts": [
			"js/lib/jquery-2.1.1.min.js",
			"js/lib/md5.js",
			"js/background.js"
		]
	},
	"browser_action": {
		"default_title": "MD5SUM",
		"default_icon": "icon.png"
	},
	"content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
	"permissions": [
		"contextMenus",
		"downloads",
		"downloads.open",
		"tabs",
		"http://*/*",
		"https://*/*"
	]
}
